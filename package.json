{
  "name": "matera",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "description": "front end project CI/CD helper",
  "author": "tjyuanpeng <tjyuanpeng@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/tjyuanpeng/matera",
  "repository": {
    "type": "git",
    "url": "https://github.com/tjyuanpeng/matera.git"
  },
  "scripts": {
    "watch:build": "tsdown --watch",
    "watch:run": "node --watch ./dist/index.mjs",
    "build": "tsdown",
    "start": "node ./dist/index.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@gitbeaker/rest": "catalog:",
    "cac": "catalog:",
    "dotenv": "catalog:",
    "fastify": "catalog:",
    "pino": "catalog:",
    "pino-pretty": "catalog:",
    "yaml": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-format": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  }
}
